
public class Sub1 {
	private int num1, num2, num3 = 0;

	public void setNum(int num1, int num2, int num3){
		this.num1 = num1;
		this.num2 = num2;
		this.num3 = num3;
	}
	
	public int getMax(){
		return Math.max(this.num1, Math.max(this.num2, this.num3));
	}
	public int getMin(){
		return Math.min(this.num1, Math.min(this.num2, this.num3));
	}
}
