/**
 * 
 */

/**
 * @author internousdev
 *
 */
public class HelloWorld {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		for(int a=1; a<=5; a++){
			for(int b=1; b<=5; b++){
				System.out.print("★");
			}
			System.out.print("\n");
		}
		
		System.out.print("\n");
		
		for(int c=1; c<=5; c++){
			for(int d=1; d<=5; d++){
				if((c+d)%2==0){
					System.out.print("★");
				}else{
					System.out.print("☆");
				}
			}
			System.out.print("\n");
		}
		
		System.out.print("\n");
		
		for(int e=1; e<=5; e++){
			for(int f=1; f<=5; f++){
				if(e==f){
					System.out.print("☆");
				}else{
					System.out.print("★");
				}
			}
			System.out.print("\n");
		}
		
		System.out.print("\n");
		
		for(int g=1; g<=5; g++){
			for(int h=1; h<=5; h++){
				if(g==h){
					System.out.print("★\n");
					break;
				}else{
					System.out.print("★");
				}
			}
		}
	}

}
