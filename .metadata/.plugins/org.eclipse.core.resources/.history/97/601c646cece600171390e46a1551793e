import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

/**
 * 
 */

/**
 * @author internousdev
 *
 */
public class Main {

	/**
	 * @param args
	 * @throws IOException 
	 */
	public static void main(String[] args) throws IOException {
		// TODO 自動生成されたメソッド・スタブ
		BufferedReader br = new BufferedReader (new InputStreamReader(System.in));
		System.out.println("0=日曜、1=月曜、2=火曜、3=水曜、4=木曜、5=金曜、6=土曜");
		System.out.print("希望の曜日：");
		int a = Integer.parseInt(br.readLine());
		
		System.out.println("0=午前、1=午後、2=夜間");
		System.out.print("希望の時間帯：");
		int b = Integer.parseInt(br.readLine());
		
		System.out.println("可能性");
		if(a == 0 || (a == 2 && b == 0) || (a == 3 && b == 2) || (a == 6 && (b == 1 || b == 2))){
			System.out.println("休診");
		}else if(a >= 7 || b >= 3){
			System.out.println("ERROR");
		}else{
			System.out.println("予約可能");
		}
		
	}

}
