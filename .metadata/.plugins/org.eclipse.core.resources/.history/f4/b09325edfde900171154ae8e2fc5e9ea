 */
public class Sub3 {
	private String name = null;
	private int age = 0;
	
	public void setName(String name){
		this.name = name;
	}
	public void setAge(int age){
		this.age = age;
	}
	
	public void showProfile(){
		System.out.print("犬の名前：" + this.name + "\n");
		System.out.print("犬の年齢：" + this.age);
	}

}
