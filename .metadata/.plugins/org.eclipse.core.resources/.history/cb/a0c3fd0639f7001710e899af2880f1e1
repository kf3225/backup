set name utf8;
set foreign_key_checks = 0;
drop database if exists logindb;
create database logindb;
use logindb;

create table user(
id int,
user_name varchar(255),
password varchar(255)
);

insert into user values
(1, "Taro", "aaaa"),
(2, "Jiro", "bbbb"),
(3, "Sabro", "cccc"),
(4, "Hanako", "dddd");